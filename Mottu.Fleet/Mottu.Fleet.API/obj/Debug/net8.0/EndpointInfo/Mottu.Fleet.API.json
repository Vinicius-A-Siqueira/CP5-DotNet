  "components": {
    "schemas": {
      "CreateMotoDto": {
        "required": [
          "modelo",
          "patioId",
          "placa"
        ],
        "type": "object",
        "properties": {
          "placa": {
            "maxLength": 20,
            "minLength": 0,
            "pattern": "^[A-Z]{3}[0-9][A-Z0-9][0-9]{2}$",
            "type": "string"
          },
          "modelo": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "patioId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "ano": {
            "maximum": 2030,
            "minimum": 2000,
            "type": "integer",
            "format": "int32"
          },
          "cor": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "quilometragem": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "chassi": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "numeroMotor": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "observacoes": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreatePatioDto": {
        "required": [
          "endereco",
          "nome"
        ],
        "type": "object",
        "properties": {
          "nome": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "endereco": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "cidade": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "estado": {
            "maxLength": 2,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "cep": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "capacidade": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "telefone": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "observacoes": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateUserDto": {
        "required": [
          "password",
          "profile",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "profile": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "fullName": {
            "maxLength": 150,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "email": {
            "maxLength": 150,
            "minLength": 0,
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "phone": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string",
            "format": "tel",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MaintenanceRequest": {
        "type": "object",
        "properties": {
          "maintenanceDate": {
            "type": "string",
            "format": "date-time"
          },
          "observations": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MotoDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "placa": {
            "type": "string",
            "nullable": true
          },
          "modelo": {
            "type": "string",
            "nullable": true
          },
          "patioId": {
            "type": "integer",
            "format": "int32"
          },
          "patioNome": {
            "type": "string",
            "nullable": true
          },
          "ano": {
            "type": "integer",
            "format": "int32"
          },
